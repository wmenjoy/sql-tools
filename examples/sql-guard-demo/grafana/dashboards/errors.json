            "type": "grafana-clickhouse-datasource",
            "uid": "clickhouse"
          },
          "format": 1,
          "rawSql": "SELECT CASE WHEN error_message LIKE '%syntax%' THEN 'Syntax Error' WHEN error_message LIKE '%constraint%' OR error_message LIKE '%duplicate%' THEN 'Constraint Violation' WHEN error_message LIKE '%deadlock%' THEN 'Deadlock' WHEN error_message LIKE '%timeout%' OR error_message LIKE '%lock wait%' THEN 'Timeout' WHEN error_message LIKE '%does not exist%' OR error_message LIKE '%doesn\\'t exist%' THEN 'Table/Column Not Found' ELSE 'Other' END as error_category, count(*) as count FROM audit.sql_executions WHERE timestamp >= now() - INTERVAL 24 HOUR AND error_message IS NOT NULL AND error_message != '' GROUP BY error_category ORDER BY count DESC",
          "refId": "A"
        }
      ],
